<h1>Listing theses</h1>

<table>
  <tr>
    <th>Item</th>
    <th>Title</th>
    <th>Tchid</th>
    <th>Stuid</th>
    <th>Summary</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @theses.each do |thesis| %>
  <tr>
    <td><%= thesis.item %></td>
    <td><%= thesis.title %></td>
    <td><%= thesis.tchId %></td>
    <td><%= thesis.stuId %></td>
    <td><%= thesis.summary %></td>
    <td><%= thesis.content %></td>
    <td><%= link_to 'Show', thesis %></td>
    <td><%= link_to 'Edit', edit_thesis_path(thesis) %></td>
    <td><%= link_to 'Destroy', thesis, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Thesis', new_thesis_path %>
